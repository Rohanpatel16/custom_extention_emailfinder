{
   "action": {
      "default_icon": {
         "128": "images/icon128.png",
         "16": "images/icon16.png",
         "24": "images/icon24.png",
         "32": "images/icon32.png"
      },
      "default_popup": "popup.html",
      "default_title": "Email & Phone Number Extractor"
   },
   "author": "Eren Geridonmez",
   "background": {
      "service_worker": "background.js"
   },
   "content_scripts": [
      {
         "matches": [
            "<all_urls>"
         ],
         "js": [
            "content.js"
         ]
      }
   ],
   "description": "Extract emails and phones from any website dynamically.",
   "icons": {
      "16": "images/icon16.png",
      "32": "images/icon32.png",
      "128": "images/icon128.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "One-Click Contact Extractor",
   "permissions": [
      "activeTab",
      "contextMenus",
      "scripting",
      "clipboardWrite",
      "storage"
   ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.1.0"
}